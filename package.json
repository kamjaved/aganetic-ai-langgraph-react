  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^24.1.0",
    "concurrently": "^9.2.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@langchain/community": "^0.3.49",
    "@langchain/core": "^0.3.66",
    "@langchain/langgraph": "^0.4.0",
    "@langchain/langgraph-checkpoint-postgres": "^0.1.0",
    "@langchain/openai": "^0.6.3",
    "@langchain/tavily": "^0.1.4",
    "@prisma/client": "^6.13.0",
    "@types/cors": "^2.8.19",
    "@types/pg": "^8.15.5",
    "axios": "^1.11.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^5.1.0",
    "pg": "^8.16.3",
    "prisma": "^6.13.0"
  },
